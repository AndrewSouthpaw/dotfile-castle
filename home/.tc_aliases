# Expected to be run from root directory
alias im='i18n-migrate'
alias imref='i18n-migrate rollback && i18n-migrate migrate && rake i18n:js:export'
alias imme='i18n-migrate migrate && rake i18n:js:export'
alias imr='i18n-migrate rollback'
alias tcps='bin/pull_school -s'
alias rs='rails server'
alias rsip='bin/rsip'
alias wp='bin/webpack_dev_server'
alias wpip='bin/wpip'
alias rdbm='bin/rdbm'
alias rdbr='bin/rdbr'

# testing
alias yt='yarn test:watch'
alias yta='yarn test'
alias lint='yarn fast-lint'

# navigation
alias tc='cd ~/Projects/TransparentClassroom'
alias tcn='cd ~/Projects/TransparentClassroom/frontend/native'

# server
alias findrails='lsof -n -i4TCP:3000'
